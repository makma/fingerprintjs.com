    "noIndexBuild": "gatsby build && echo 'User-agent: *\nDisallow: /' > public/robots.txt",
    "develop": "yarn clean && gatsby develop",
    "playground": "GATSBY_GRAPHQL_IDE=playground gatsby develop",
